#[forecast]
#command=~/.config/i3blocks/forecast.sh
#interval=3600

[network]
command=~/.config/i3blocks/network.sh
interval=5
interface=wlp1s0

[volume]
command=echo -e "\uf027 $(amixer -c 0 get Master | awk '/Mono: / {print $4 $6}' | sed -r "s/\[([0-9]+%)\]\[(\w+)\]/\1(\2)/g")"
interval=3

#[mpd]
#command=echo -e "\uf001 $(ruby ~/.config/i3blocks/mpd.rb)"
#interval=5

[battery]
command=~/.config/i3blocks/battery.sh
interval=10

[time]
command=echo -e "\uf017 $(date +"%F %R %Z")"
interval=20
